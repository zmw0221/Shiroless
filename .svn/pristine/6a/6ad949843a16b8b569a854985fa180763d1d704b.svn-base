<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
  <head>
    <title>crud.html</title>
	
    <meta http-equiv="keywords" content="keyword1,keyword2,keyword3">
    <meta http-equiv="description" content="this is my page">
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <link rel="stylesheet" type="text/css" href="themes/default/easyui.css">
	<link rel="stylesheet" type="text/css" href="themes/icon.css">
	<script type="text/javascript" src="jquery.min.js"></script>
	<script type="text/javascript" src="jquery.easyui.min.js"></script>
   <script type="text/javascript">
   		//添加tabs
   		function urlClick(){
   			//判断是否存在学生管理tabs
   			var isExist= $("#tb").tabs("exists","食品管理");
   			if(!isExist){
   				$("#tb").tabs("add",{
   					title:'食品管理',
   					closable:true,
   					content:'<iframe frameborder=0 width="100%" height="100%" scrolling="no" src="crud.html"></iframe>'
   				})
   			}
   			$("#tb").tabs("select","食品管理");
   		}
   		
   </script>

  </head>
  
  <body style="padding:1px;margin:1px">
  	
  	<div class="easyui-layout" style="width:100%;height:100%;">
  		<!-- 上北只能设置高度 一般不会设置宽度 -->
		<div data-options="region:'north'" style="height:15%"></div>
		<div data-options="region:'west',split:true" title="导航菜单" style="width:17%;">
			<!-- 添加菜单栏 -->
			<div class="easyui-accordion" style="width:95%;height:100%;">
				<div title="权限管理"  style="overflow:auto;padding:10%;">
					<a href="javascript:urlClick()" style="text-decoration: none "><img src="themes/icons/man.png"/>食品管理</a>
				</div>
				<div title="系统管理" style="padding:10%;">
					
				</div>
			</div>	
		</div>
		<div data-options="region:'center',iconCls:'icon-ok'">
			<!-- 添加内容 -->
			<div id="tb" class="easyui-tabs" style="width:100%;height:100%">
				<div title="欢迎使用" style="padding:10px ">
				<iframe frameborder=0 width="100%" height="100%" scrolling="no" src="tr.html"></iframe>
				</div>
				
			</div>	
		</div>
	</div>
  
  </body>
</html>
